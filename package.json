{
  "name": "daba",
  "version": "0.0.24",
  "description": "Setup and deploy web apps in minutes.",
  "main": "./out/main/index.js",
  "author": "daba.so",
  "homepage": "https://www.daba.so",
  "scripts": {
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:node:watch": "tsc --watch --noEmit -p tsconfig.node.json --composite false",
    "svelte-check": "svelte-check --tsconfig ./tsconfig.json --compiler-warnings \"a11y_click_events_have_key_events:ignore,a11y_no_static_element_interactions:ignore,a11y_autofocus:ignore\"",
    "svelte-check:watch": "svelte-check --watch --tsconfig ./tsconfig.json --compiler-warnings \"a11y_click_events_have_key_events:ignore,a11y_no_static_element_interactions:ignore,a11y_autofocus:ignore\"",
    "typecheck": "npm run typecheck:node && npm run svelte-check",
    "typecheck:watch": "npm run typecheck:node:watch",
    "start": "electron-vite preview",
    "dev": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "npm run build && electron-builder --mac --config -- --publish=never",
    "build:linux": "npm run build && electron-builder --linux --config",
    "publish": "npm version patch -m 'Upgrade to %s' && git push origin main --follow-tags"
  },
  "repository": "https://github.com/dabateam/daba",
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "animejs": "^3.2.2",
    "clsx": "^2.1.1",
    "dockerode": "^4.0.2",
    "electron-log": "^5.1.5",
    "electron-window-state": "^5.0.3",
    "fs-extra": "^11.2.0",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "prettier-plugin-svelte": "^3.2.3",
    "tailwind-merge": "^2.3.0",
    "tar-fs": "^3.0.6",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@kjn/electron-typesafe-ipc": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@tsconfig/svelte": "^5.0.2",
    "@types/animejs": "^3.1.12",
    "@types/dockerode": "^3.3.29",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "16.18.21",
    "@types/tar-fs": "^2.0.4",
    "autoprefixer": "^10.4.19",
    "electron": "^22.3.4",
    "electron-builder": "^23.6.0",
    "electron-updater": "^6.2.1",
    "electron-vite": "^2.3.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "svelte": "^5.0.0-next.121",
    "svelte-check": "^3.6.7",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
